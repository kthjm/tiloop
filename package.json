{
  "name": "tiloop",
  "version": "0.0.2",
  "repository": "kthjm/tiloop",
  "author": "kthjm <toxictoxer@gmail.com>",
  "license": "MIT",
  "description":
    "Create iterator done coincident with covering all index of virtual array.",
  "keywords": ["iterator", "iterable", "array", "api"],
  "main": "./cjs.js",
  "jsnext:main": "./es.js",
  "module": "./es.js",
  "prettier": {
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true
  },
  "nyc": {
    "require": ["babel-register"],
    "sourceMap": false,
    "instrument": false
  },
  "scripts": {
    "pretest": "flow",
    "test": "cross-env NODE_ENV=TEST nyc mocha ./test.js",
    "prebuild": "yarn test",
    "build": "cross-env NODE_ENV=BUILD node rollup.rollup.js",
    "postbuild": "yarn format",
    "format": "prettier --write **/*.js package.json",
    "coverage": "nyc report --reporter=lcov && codecov"
  },
  "dependencies": {
    "regenerator-runtime": "^0.11.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-rewire": "^1.1.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-power-assert": "^1.0.0",
    "codecov": "^3.0.0",
    "cross-env": "^5.1.3",
    "flow-bin": "^0.63.1",
    "flow-typed": "^2.2.3",
    "mocha": "^4.1.0",
    "npm-run-all": "^4.1.2",
    "nyc": "^11.4.1",
    "power-assert": "^1.4.4",
    "prettier": "^1.10.2",
    "rewire": "^3.0.2",
    "rollup": "^0.54.0",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-flow": "^1.1.1",
    "sinon": "^4.1.5"
  }
}
